    }

    HAL_Delay( 5 );

	if((error =3D I2C_MasterRead(I2C_MASTER, I2C_SLAVE_ADDR, data, bytes, 0)) =
!=3D bytes) {
		printf("%s:%d Error writing: %d\n", __func__, __LINE__, error);
	}

    HAL_Delay( BQ27421_DELAY );

    return true;
}



