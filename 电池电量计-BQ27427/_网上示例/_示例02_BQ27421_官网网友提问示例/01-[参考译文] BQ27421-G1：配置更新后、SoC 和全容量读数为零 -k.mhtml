tes )
{
    HAL_StatusTypeDef ret;

    uint8_t i2c_data;

    i2c_data =3D BQ27421_BLOCK_DATA_START + offset;

 	if((error =3D I2C_MasterWrite(I2C_MASTER, I2C_SLAVE_ADDR, &amp;i2c_data, =
1, 0)) !=3D 1) {
