bool bq27421_i2c_write_data_block( uint8_t offset, uint8_t *data, uint8_t b=
ytes )
{
    HAL_StatusTypeDef ret;
    uint8_t i2c_data[2], i;

    for( i =3D 0; i &lt; bytes; i++ )
    {
        i2c_data[0] =3D BQ27421_BLOCK_DATA_START + offset + i;
        i2c_data[1] =3D data[i];

     	if((error =3D I2C_MasterWrite(I2C_MASTER, I2C_SLAVE_ADDR, i2c_data, 2=
, 0)) !=3D 2) {
        		printf( "%s:%d Error writing: %d\n", __func__, __LINE__, error);
        }
        HAL_Delay( BQ27421_DELAY );
    }

    return true;
}

